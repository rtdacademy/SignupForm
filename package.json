  "scripts": {
    "cleanup:all": "kill-port 3000 9099 5001 8080 8765 5000 9199 4400 4401 4000 4001 4500 4501 9299 9300 9499 9500",
    "start:emulators:functions-only": "firebase emulators:start --only functions",
    "start:main": "node scripts/build.js --start",
    "start:main:functions-emulator": "REACT_APP_USE_FUNCTIONS_EMULATOR=true node scripts/build.js --start",
    "start:main:functions-emulator:win": "cross-env REACT_APP_USE_FUNCTIONS_EMULATOR=true node scripts/build.js --start",
    "start:second": "node scripts/build.js --second-site --start",
    "start:second:functions-emulator": "REACT_APP_USE_FUNCTIONS_EMULATOR=true node scripts/build.js --second-site --start",
    "start:second:functions-emulator:win": "cross-env REACT_APP_USE_FUNCTIONS_EMULATOR=true node scripts/build.js --second-site --start",
    "start:rtdconnect": "node scripts/build.js --rtd-connect --start",
    "start:rtdconnect:functions-emulator": "REACT_APP_USE_FUNCTIONS_EMULATOR=true node scripts/build.js --rtd-connect --start",
    "start:rtdconnect:functions-emulator:win": "cross-env REACT_APP_USE_FUNCTIONS_EMULATOR=true node scripts/build.js --rtd-connect --start",
    "start:main:win": "node scripts/build.js --start",
    "start:second:win": "node scripts/build.js --second-site --start",
    "start:rtdconnect:win": "node scripts/build.js --rtd-connect --start",
    "genkit:ui": "cd functions && GENKIT_ENV=dev npx genkit start -o -- node local-dev-server.js",
    "genkit:ui:win": "cd functions && cross-env GENKIT_ENV=dev npx genkit start -o -- node local-dev-server.js",
    "start:main:functions-genkit": "concurrently \"npm run start:emulators:functions-only\" \"npm run genkit:ui\" \"REACT_APP_USE_FUNCTIONS_EMULATOR=true npm run start:main\"",
    "start:main:functions-genkit:win": "concurrently \"npm run start:emulators:functions-only\" \"npm run genkit:ui:win\" \"npm run start:main:functions-emulator:win\"",
    "build:main": "node scripts/build.js",
    "build:second": "node scripts/build.js --second-site",
    "build:rtdconnect": "node scripts/build.js --rtd-connect",
    "deploy:main": "npm run build:main && firebase deploy --only hosting:main",
    "deploy:second": "npm run build:second && firebase deploy --only hosting:second-site",
    "deploy:rtdconnect": "npm run build:rtdconnect && firebase deploy --only hosting:rtd-connect",
    "clean": "node scripts/build.js --clean-only",
    "prebuild": "node scripts/build.js --clean-only",
    "create-course": "node scripts/create-course.js",
    "generate-slideshow": "node scripts/generate-slideshow-questions.js",
    "create-course-from-structure": "node scripts/create-course.js",
    "delete-course": "node scripts/delete-course.js",
    "prepare-shared": "node scripts/prepare-shared.js",
    "deploy:course-2a": "npm run prepare-shared && cd functions/courses/2a && npm install && firebase deploy --only functions:course-2a",
    "deploy:course-2a2": "npm run prepare-shared && cd functions/courses/2a2 && npm install && firebase deploy --only functions:course-2a2",
    "deploy:course-2b": "npm run prepare-shared && cd functions/courses/2b && npm install && firebase deploy --only functions:course-2b",
    "deploy:course-2c": "npm run prepare-shared && cd functions/courses/2c && npm install && firebase deploy --only functions:course-2c",
    "deploy:course-3": "npm run prepare-shared && cd functions/courses/3 && npm install && firebase deploy --only functions:course-3",
    "deploy:course-4": "npm run prepare-shared && cd functions/courses/4 && npm install && firebase deploy --only functions:course-4",
    "gemini": "env-cmd -f .env npx @google/gemini-cli"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
